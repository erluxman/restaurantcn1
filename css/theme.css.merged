
/* /Users/adventure/Desktop/Restaurant Demo/css/theme.css */
#Constants {
    includeNativeBool: true;
}
@font-face {
    font-family: "righteous";
    src: url(fonts/Righteous.ttf);
}
@font-face {
    font-family: "airborne";
    src: url(fonts/Airborne.ttf);
}

@font-face {
    font-family: "sen";
    src: url(fonts/Sen.ttf);
}

@font-face {
    font-family: "rex";
    src: url(fonts/Rex.ttf);
}

:root {
   --styled-text: "rex";
   --normal-text: "sen";
 }


SplashScreen {
        background-image: url("images/background.png");
        cn1-source-dpi: 0;
        cn1-background-type: cn1-image-scaled-fill;
}

Logo {
        background-image: url("images/logo.png");
        cn1-source-dpi: 250;
}
Logo {
        background-image:
        url("images/border_decoration.png"),
        url("images/border_decoration_red.png"),
        url("images/border_decoration_green.png"),
        url("images/decorationleft.png"),
        url("images/decorationright.png"),
        url("images/background.png"),
        url("images/coffee.png"),
        url("images/contactus.png"),
        url("images/findus.png"),
        url("images/address.png"),
        url("images/foodmenu.png"),
        url("images/kattiroll.png"),
        url("images/logo.png"),
        url("images/map.png"),
        url("images/mobile.png"),
        url("images/momo.png"),
        url("images/pizza.png"),
        url("images/weareopen.png"),
        url("images/reservation.png"),
        url("images/salad.png"),
        url("images/sausage.png"),
        url("images/seafood.png"),
        url("images/thakali1.png"),
        url("images/thakali2.png");
}

/* Toolbar Menu*/
MenuToolbar {
        margin-top: 30px;
}

MenuTitleLabel{
        color: white;
        font-size: 16pt;
        margin: 30px;
        text-align: center;
        font-family: var(--styled-text);
        margin-left : 100px;
}

MenuTile {
        margin: 96px;
}

MenuTileLabel {
        cn1-derive: MenuTitleLabel;
        font-size: 10pt;
}

FoodMenu {
        background-image: url("images/foodmenu.png");
        cn1-source-dpi: 250;
}

ContactUs {
        background-image: url("images/contactus.png");
        cn1-source-dpi: 250;
}

FindUs {
        background-image: url("images/findus.png");
        cn1-source-dpi: 250;
}

Reservation {
        background-image: url("images/reservation.png");
        cn1-source-dpi: 250;
}

WhiteIcon {
        color: white;
        margin: 50px 100px 250px 100px;
}

/*MenuScreen Starts */
MenuScreen {
        background-color: black;
        color: black;
}

MenuCard{
        margin : 50px 200px 50px 30px;
        padding :10px;
        background-image:url("images/dashbg_landscape.png");
        cn1-background-type: cn1-image-scaled;
}
MenuCardWhole{
        padding-top : 100px;
        padding-bottom : 100px;
}
FoodInfo{
        margin-top: 50px;
        padding-right: 300px;
        padding-left: 10px;
}

MenuCardImage{
        margin-right: 0px;
}

FoodTitle{
        color: #CB433A;
        font-family: var(--styled-text);
        font-size: 12pt;
}

FoodDescription{
        color: #cccccc;
        font-size: 7pt;
        font-family: var(--normal-text);
}

MenuCardPrice{
       background: cn1-round-border;
       color: white;
       font-size: 6pt;
       background: #0B803B;
       text-align:center;
       padding:1.8mm;
}
