/*Styles for  Food details Page*/

FoodDetailsTitle{
        color: #CB433A;
        font-size: 20pt;
        text-align: center;
        font-family: var(--styled-text);
}

FoodDetailsDescription{
        color: white;
        font-size: 10pt;
        font-family: var(--normal-text);
        text-align: center;
}

GreenLine{
        margin-top: 48pt;
}

ItemPrice{
       background: cn1-round-border;
       color: white;
       font-size: 10pt;
       background: #338833;
       margin-left : 2mm;
       margin-right : 2mm;
       text-align:center;
       padding:2mm;
}


ItemDetailsInfo{
        padding: 2mm;
        background-image:url("images/dashbg_potrait.png");
        cn1-background-type: cn1-image-scaled;
        cn1-9patch: 5px 8px 4px 10px;
        padding-top: 20mm;
        margin :20px;
        margin-top: 15mm;
}

FoodDetailsGoBack {
        background-color: #CB433A;
        background-repeat: space;
        color: white;
        padding: 2mm;
        text-align: center;
        border-radius: 50px;
        margin: 200px 100px 150px 100px;
}


/*Contact Us Page*/

ItemTitle{
        color: #ff5555;
        font-size: 16pt;
        text-align: center;
        margin-top: 5mm;
}

ShcheduleRowText{
        color: #eeeeee;
        font-size: 8pt;
        text-align: left;
}

ShcheduleRowDashText{
        color: #eeeeee;
        font-size: 8pt;
        text-align: center;
}

/*Find Us page*/
AddressCard{
        background:#cccccc;
}

RedIcon {
        color: #aa3333;
}

PhoneCard {
        margin-left: 50px;
}

NormalWhiteText{
        color: #666666;
        font-size: 6pt;
}

/*Splash Screen*/
SplashButton {
        background-color: #CB433A;
        background-repeat: space;
        color: white;
        padding: 2mm;
        text-align: center;
        border-radius: 50px;
        margin: 0px 100px 150px 100px;
        font-family: var(--styled-text);
}
/*Reservation Screen*/

BookScreen {
        background-image: url("images/reservation_bg.png");
        cn1-source-dpi: 450;
        cn1-background-type: cn1-image-scaled-fill;
        background-color:black;
}

PickerTitle {
        font-size: 15pt;
        margin-top: 10mm;
        text-align: left;
        font-family: var(--styled-text);
        color: white;
}

PickerLabel {
        color: white;
        font-size: 9pt;
        text-align: left;
        font-weight: bold;
        margin: 10px;
}

TextComponent{
        background-color: rgba(0, 0, 0, 0.0);
}

TextComponentField{
        color: white;
}

DescriptionLabel{
        color: white;
}

DropdownStyle{
        background-color: #333333;
        color: white;
}

Picker {
        background-color: black;
        color: white;
}

Picker.selected {
        background-color: black;
        color: white;
}
Picker.pressed {
        background-color: black;
        color: white;
}
Picker.disabled {
        background-color: black;
        color: white;
}

BookScreenGoBack {
        color: white;
        text-align: center;
        font-size:10pt;
        font-family: var(--styled-text);
}
/*Shared Styles*/
RedPillButton {
        background-color: #CB433A;
        background-repeat: space;
        color: white;
        padding: 1mm;
        text-align: center;
        border: 0pt #3399ff cn1-pill-border;
        margin: 0px 50px 50px 50px;
        font-family: var(--styled-text);
}

PillButtonText{
        margin-top: 1mm;
        color: white;
        font-size: 14pt;
        font-weight: bold;
        font-family: var(--styled-text);
}


AppTitleLabel {
        color: white;
        font-size: 24pt;
        margin-top: 15mm;
        font-weight: bold;
        text-align: center;
        font-family: var(--styled-text);
}

AppTitleLabelMini {
        color: white;
        font-size: 16pt;
        font-family: var(--styled-text);
        text-align: center;
}

AppTitleLabelMedium {
        color: white;
        font-size: 18pt;
        font-family: var(--styled-text);
        text-align: center;
        margin: 50px 100px 150px 100px;
}


